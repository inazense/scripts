{
	"Jekyll code": {
		"prefix": "coding",
		"body": [
			"{% highlight $1 %}",
			"$2",
			"{% endhighlight %}",
		],
		"description": "Add jekyll code snippet"
	},
	"Jekyll img": {
		"prefix": "image",
		"body": "![$1]($2)",
		"description": "Jekyll image snippet"
	},
	"Jekyll site baseurl": {
		"prefix": "site",
		"body": "{{ site.baseurl }}",
		"description": "Jekyll site baseurl"
	},
	"Jekyll link": {
		"prefix": "link",
		"body": "[$1]($2){:target=\"blank\"}",
		"description": "Jekyll link snippet"
	}
}
